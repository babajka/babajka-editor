{
  "name": "@babajka/editor",
  "version": "0.0.0-semantically-released",
  "description": "another rich text slate editor",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "dev": "cd examples/ssr-with-next && npm run dev",
    "add-contributor": "kcd-scripts contributors add",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "slate",
    "editor",
    "rich"
  ],
  "author": "Ivan Pazhitnykh <ivan@wir.by>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=0.14.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "immutable": "^4.0.0-rc.12",
    "is-hotkey": "^0.1.4",
    "react-render-html": "^0.6.0",
    "slate": "^0.45.1",
    "slate-html-serializer": "^0.7.39",
    "slate-react": "^0.21.21"
  },
  "devDependencies": {
    "kcd-scripts": "^0.49.0",
    "next": "^8.0.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/babajka/editor.git"
  },
  "bugs": {
    "url": "https://github.com/babajka/editor/issues"
  },
  "homepage": "https://github.com/babajka/editor#readme"
}
