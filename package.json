{
  "name": "babajka-editor",
  "version": "0.0.0-semantically-released",
  "description": "draft.js editor",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "add-contributor": "kcd-scripts contributors add",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Ivan Pazhitnykh <ivan@wir.by>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=0.14.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "draft-js": "^0.10.5"
  },
  "devDependencies": {
    "kcd-scripts": "^0.49.0",
    "next": "^8.0.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/babajka/babajka-editor.git"
  },
  "bugs": {
    "url": "https://github.com/babajka/babajka-editor/issues"
  },
  "homepage": "https://github.com/babajka/babajka-editor#readme"
}
